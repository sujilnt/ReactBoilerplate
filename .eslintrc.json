{
    "env": {
        "browser": true,
        "amd": true,
        "es6": true,
        "node": true
    },
    "extends":[
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "allow": ["log","warn", "error"],
        "no-console": "off"
    },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "16.0"
        }
    }
}
